      color: #555;
      display: flex; /* Use flexbox to arrange the tools horizontally */
      flex-wrap: wrap; /* Allow the bubbles to wrap to the next line if necessary */
      gap: 10px; /* Add space between the bubbles */
    }

     .tools_odd {
      font-size: 1.1rem;
      color: #555;
      display: flex; /* Use flexbox to arrange the tools horizontally */
      flex-wrap: wrap; /* Allow the bubbles to wrap to the next line if necessary */
      gap: 10px; /* Add space between the bubbles */
    }

    .tools .tool {
      background-color: #007bff; /* A blue color for the bubble */
      color: white; /* Text color inside the bubble */
      padding: 5px 12px; /* Add some padding for the bubble effect */
      border-radius: 12px; /* Round the edges of the bubble */
      font-weight: bold; /* Make the text bold */
      font-size: 0.9rem; /* Ensure the text is a readable size */
      box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1); /* Add a subtle shadow for the bubble effect */
      transition: transform 0.3s ease; /* Add a transition for hover effect */
    }

    .tools_odd .tool {
      background-color: #07b9fa; /* A blue color for the bubble */
      color: black; /* Text color inside the bubble */
      padding: 5px 12px; /* Add some padding for the bubble effect */
      border-radius: 12px; /* Round the edges of the bubble */
      font-weight: bold; /* Make the text bold */
      font-size: 0.9rem; /* Ensure the text is a readable size */
      box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1); /* Add a subtle shadow for the bubble effect */
      transition: transform 0.3s ease; /* Add a transition for hover effect */
    }

    .tools .tool:hover {
      transform: scale(1.1); /* Slightly enlarge the bubble when hovered */
    }

    .description p {
      margin: 0;
    }

    .repo-link {
      margin-top: 10px;
    }

    .repo-link a {
      display: inline-block; /* Make the link behave like a block for better control */
      padding: 5px 12px; /* Add padding around the text */
      background-color: #064028; /* Set background color */
      color: #44dbd6; /* Text color */
      font-size: 0.9rem; /* Font size */
      text-decoration: none; /* Remove underline */
      border-radius: 12px; /* Rounded corners */
      box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1); /* Optional: Add a subtle shadow */
      transition: background-color 0.3s ease, transform 0.2s ease; /* Smooth transitions */
      font-family: "Monaco", monospace;
    }

    .repo-link a:hover {
      background-color: #0056b3; /* Darken the background on hover */
      transform: scale(1.05); /* Slightly enlarge the link on hover */
    }

    /* Right section - Project images */
    .project-images {
      flex: 0 0 600px;
      display: flex;
      flex-direction: column;
      gap: 10px;
      align-items: flex-start; /* Align images to the left */
      margin-left: 20px;
    }

    /* Big image */
    .big-image {
      width: 100%;
      max-height: 450px;
      object-fit: cover;
      border-radius: 10px;
      box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
      margin-bottom: 20px;
    }

    /* Small images */
    .small-images {
      display: grid;
      grid-template-columns: repeat(2, 1fr);
      gap: 10px;
      width: 80%;
      margin-left: 60px;
    }

    .small-images img {
      width: 100%;
      height: 100px;
      object-fit: cover;
      border-radius: 8px;
      cursor: pointer;
      transition: transform 0.3s ease;
    }

    .small-images img:hover {
      transform: scale(1.1);
    }

    .about-me-container {
      display: flex;
      align-items: center; /* Vertically align content */
      justify-content: center; /* Horizontally center the section */
      padding: 40px 20px;
      background-color: #efd9fc; /* Light gray background */
      margin-bottom: 40px; /* Space between the About Me and project containers */
    }

    .about-me-image {
      flex: 1; /* Make the image take up 1/3 of the space */
      margin-right: 20px; /* Space between the image and text */
      text-align: center;
    }

    .profile-picture {
      width: 100%; /* Ensure the image is responsive */
      max-width: 250px; /* Limit the maximum width */
      height: auto;
      border-radius: 50%; /* Circular profile picture */
      margin-right: 100px;
    }

    .github-link, .resume-link {
      margin-top: 10px; /* Space between image and GitHub link */
      margin-right: 100px;
    }

    .github-icon, .resume-icon {
      font-size: 1.2rem;
      color: #333;
      text-decoration: none;
      display: inline-flex;
      align-items: center;
      justify-content: center;
      gap: 8px; /* Space between the icon and text */
    }

    .github-icon:hover, .resume-icon:hover {
      color: #0366d6; /* GitHub blue color on hover */
    }

    .about-me-content {
      flex: 2; /* Make the content take up 2/3 of the space */
      max-width: 600px;
      margin-right: 100px;
    }

    .about-me-content h2 {
      font-size: 2rem;
      margin-bottom: 20px;
      text-shadow: none;
    }

    .about-me-content p {
      font-size: 1.1rem;
      color: #555;
      line-height: 1.6;
    }
